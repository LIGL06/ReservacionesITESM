extends ../layout.jade
block contenido  
  meta(http-equiv='Content-type', content='text/html; charset=utf-8')
  script(src='/js/dhtmlxscheduler.js', type='text/javascript', charset='utf-8')
  link(rel='stylesheet', href='/css/dhtmlxscheduler.css', type='text/css', media='screen', title='no title', charset='utf-8')
  style(type='text/css', media='screen').
      html, body{
      margin:0px;
      padding:0px;
      height:100%;
      overflow:hidden;
      }
  script(type='text/javascript', charset='utf-8').
    function init() {
    scheduler.init('scheduler_here',new Date(2016,1,1),"month");
    }
  body(onload='init();')
      #scheduler_here.dhx_cal_container(style='width:100%; height:100%;')
        .dhx_cal_navline
          .dhx_cal_prev_button &nbsp;
          .dhx_cal_next_button &nbsp;
          .dhx_cal_today_button
          .dhx_cal_date
          .dhx_cal_tab(name='day_tab', style='right:204px;')             
          .dhx_cal_tab(name='week_tab', style='right:140px;')
          .dhx_cal_tab(name='month_tab', style='right:76px;')
        .dhx_cal_header
        .dhx_cal_data
